algoritmo "impaparprocedimento"
var
   N : Inteiro
procedimento parouimpar (a : inteiro)
inicio
      se (a % 2 = 0) entao
         escreva("Seu número é PAR!")
      senao
         escreva("Seu número é IMPAR!")
      fimse
fimprocedimento
inicio
      escreva("Digite um numero: ")
      leia(N)
      parouimpar(N)
      escreval(a)
      escreval(n)
fimalgoritmo